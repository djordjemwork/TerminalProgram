<?import javafx.scene.layout.VBox?>

<AnchorPane stylesheets="@/application/style/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainController">
    <BorderPane prefHeight="720" prefWidth="1042.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">

        <rotationAxis>
            <Point3D />
        </rotationAxis>
        <center>
            <GridPane prefHeight="543.0" prefWidth="642.0" BorderPane.alignment="CENTER">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints maxHeight="264.0" minHeight="0.0" prefHeight="44.0" vgrow="SOMETIMES" />
                    <RowConstraints maxHeight="517.0" minHeight="10.0" prefHeight="485.0" vgrow="SOMETIMES" />
                    <RowConstraints maxHeight="109.0" minHeight="0.0" prefHeight="53.0" vgrow="SOMETIMES" />
                </rowConstraints>
                <children>
                    <TableView style="-fx-focus-color: transparent;" editable="true" fx:id="tableUserAccounts" prefHeight="513.0" prefWidth="642.0" GridPane.rowIndex="1">
                        <columns>
                            <TableColumn fx:id="columnUserName" maxWidth="321" minWidth="321" style="-fx-alignment: CENTER" text="Username" />
                            <TableColumn fx:id="columnPassword" maxWidth="330" minWidth="330" style="-fx-alignment: CENTER" text="Password" />
                        </columns>
                    </TableView>
                </children>
            </GridPane>
        </center>
        <left>
            <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
        </left>
        <right>
            <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
        </right>
        <top>
            <VBox prefHeight="126.0" prefWidth="1042.0" BorderPane.alignment="CENTER">
                <children>
                    <HBox prefHeight="15.0" prefWidth="1042.0">
                        <children>
                            <ProgressBar fx:id="progressBarMain" styleClass="progress-bar" maxHeight="15" minHeight="15" prefHeight="15" prefWidth="1055.0" />
                        </children>
                    </HBox>
                    <ToolBar prefHeight="97.0" prefWidth="1042.0" style="-fx-background-color: none">
                        <items>
                            <Pane HBox.hgrow="ALWAYS" />
                            <Button fx:id="btnAddNewAccount" mnemonicParsing="false" prefHeight="40.0" prefWidth="160.0" text="ADD NEW" />
                            <Pane HBox.hgrow="ALWAYS" />
                            <Button fx:id="btnDeleteAccount" mnemonicParsing="false" prefHeight="40.0" prefWidth="160.0" text="DELETE" />
                            <Pane HBox.hgrow="ALWAYS" />
                            <Button fx:id="btnSaveToCard" mnemonicParsing="false" prefHeight="40.0" prefWidth="160.0" text="SAVE TO CARD" />
                            <Pane HBox.hgrow="ALWAYS" />
                        </items>
                    </ToolBar>
                </children>
            </VBox>
        </top>
    </BorderPane>
</AnchorPane>
